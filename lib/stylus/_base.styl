// @font-face
//   font-family GameFont
//   font-style normal
//   src url(./fonts/font.ttf)

img 
  display block
  width 100%
  height 100%
  object-fit cover
  
em
i
  font-style normal
  font-weight normal

p
  text-align justify
  line-height 1
a
  text-decoration none
a:active
a:focus
  outline none
a:link
a:visited
a:hover
a:active 
  color rgba(0,0,0,0)

table
  border-spacing 0
  border-collapse collapse

h1
h2
h3
h4
h5
h6
p 
  line-height 1
  font-weight normal
  font-style normal

nav
ul li
ol li 
  list-style none

body 
  width 100%
  font-family Helvetica, microsoft yahei, Arial, Verdana, sans-serif
  text-size-adjust none
  
input
textarea
button
select {
	background none
	border-radius: 0
  border none
	outline none
	-webkit-appearance none
	-webkit-user-select text
	-webkit-tap-highlight-color rgba(0, 0, 0, 0)
	// user-select none
  // touch-callout none
}

input::-webkit-outer-spin-button
input::-webkit-inner-spin-button 
	-webkit-appearance: none
*
  padding 0
  margin 0
  box-sizing border-box
